{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "AzureDoom, Gecko, Eliot, Chappie, DerToaster, Tslat, Bvanseg"
  ],
  "contact": {
    "homepage": "${mod_url}",
    "issues": "${mod_issues}",
    "sources": "${mod_sources}"
  },
  "license": "${mod_license}",
  "icon": "${mod_logo}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "mod.azure.doom.FabricMCDoomMod"
    ],
    "client": [
      "mod.azure.doom.ClientListener"
    ],
    "rei_client": [
      "mod.azure.doom.rei.ReiPlugin"
    ]
  },
  "mixins": [
    "doom.mixins.json",
    "doom.fabric.mixins.json"
  ],
  "accessWidener": "doom.aw",
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "java": ">=17",
    "minecraft": "${minecraft_version}",
    "azurelib": ">=2.0.9",
    "smartbrainlib": ">=1.11",
    "trinkets": ">=3.7.0"
  }
}