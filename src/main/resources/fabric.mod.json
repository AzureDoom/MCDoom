{
	"schemaVersion": 1,
	"id": "doom",
	"version": "${version}",
	"name": "Doom Fabric",
	"description": "This is my Fabric port of my Forge Doom mod!",
	"authors": [
		"AzureDoomC"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/azuredooms-doom-mod-fabric/",
		"issues": "https://github.com/AzureDoom/MCDoom/issues",
		"sources": "https://github.com/AzureDoom/MCDoom/"
	},
	"license": "CC-BY-NC-4.0",
	"icon": "assets/doom/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"mod.azure.doom.DoomMod"
		],
		"client": [
			"mod.azure.doom.client.ClientInit"
		],
		"modmenu": [
			"mod.azure.doom.config.ModMenuDoom"
		]
	},
	"mixins": [
		"doom.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.11.7",
		"fabric": "*",
		"minecraft": "1.18.x",
		"geckolib3": ">=3.0.6"
	},
	"accessWidener": "doom.accesswidener"
}
